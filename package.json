{
  "name": "domator",
  "version": "1.1.1",
  "description": "Minimal & Fast DOM creation.",
  "main": "domator.js",
  "scripts": {
    "test": "mocha --require babel-core/register",
    "compile": "rollup -c && browserify domator.js -s domator -o dist/domator.js && uglifyjs dist/domator.js > dist/domator.min.js",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mjlescano/domator.git"
  },
  "keywords": [
    "DOM",
    "elements",
    "create",
    "createElement"
  ],
  "author": "@mjlescano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjlescano/domator/issues"
  },
  "homepage": "https://github.com/mjlescano/domator#readme",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "browserify": "^13.0.0",
    "browserify-umdify": "^1.0.3",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "mocha-standard": "^1.0.0",
    "rollup": "^0.34.1",
    "rollup-plugin-babel": "^2.6.1",
    "standard": "^5.4.1",
    "uglifyify": "^3.0.1"
  },
  "dependencies": {
    "array-foreach": "^1.0.1",
    "deepmerge": "^0.2.10",
    "is-array": "^1.0.1"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "define"
    ],
    "ignore": [
      "domator.js",
      "dist"
    ]
  }
}
